{
  "name": "electron-vue-tailwindcss-starter-kit",
  "version": "1.0.0",
  "description": "Electron Vue Tailwind Starter Kit",
  "main": "main.js",
  "scripts": {
    "start": "electron . --dev",
    "postinstall": "electron-rebuild install-app-deps",
    "pack": "electron-builder --dir ./server/main.js",
    "dist": "electron-builder ./server/main.js"
  },
  "repository": "https://github.com/Summonshr/electron-vue-tailwindcss-starter.git",
  "keywords": [
    "Electron",
    "tailwindcss",
    "vue",
    "starter",
    "kit"
  ],
  "build": {
    "appId": "Electron Vue Tailwind Starter Kit",
    "productName": "Electron Vue Tailwind Starter Kit",
    "icon": "./images/icon.jpg"
  },
  "author": "Suman Shrestha",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^7.1.2",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "electron-reload": "^1.5.0",
    "tailwindcss": "^1.1.3",
    "vue": "^2.6.10"
  }
}
